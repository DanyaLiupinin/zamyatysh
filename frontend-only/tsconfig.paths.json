{
    "compilerOptions": {
        "baseUrl": "./src",
        "paths": {
            "@hooks": [
                "shared/index.tsx"
            ],
            "@features": [
                "features/index.tsx"
            ],
            "@widgets": [
                "widgets/index.tsx"
            ],
            "@pages": [
                "pages/index.tsx"
            ],
            "@handlers": [
                "shared/lib/handlers/index.tsx"
            ],
            "@components": [
                "shared/ui/components/index.tsx"
            ],
            "@constants": [
                "shared/constants/index.tsx"
            ],
            "@types": [
                "shared/types/index.tsx"
            ],
            "@icons/*": [
                "shared/ui/assets/icons/*"
            ],
            "@images/*": [
                "shared/ui/assets/images/*"
            ],
            "@mixins": [
                "app/styles/mixins/mixins.scss"
            ],
            "@api": [
                "shared/utils/api.ts"
            ],
            "@store": [
                "app/store/index.ts"
            ],
            "@locale/*": [
                "app/locale/*"
            ]
        }
    }
}